<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="1095dp"
        android:layout_height="1498dp"
        app:srcCompat="@drawable/myimage"
        tools:layout_editor_absoluteX="-10dp"
        tools:layout_editor_absoluteY="23dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="712dp"
        android:layout_height="328dp"
        android:layout_marginStart="212dp"
        android:layout_marginLeft="212dp"
        android:fontFamily="sans-serif-medium"
        android:text='"The greatest successes come from having the freedom to fail." - Mark Zuckerberg'
        android:textColor="#FFFFFF"
        android:textSize="60dp"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="513dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="222dp"
        android:layout_height="90dp"
        android:background="#FFAA00"
        android:text="Let's Go!"
        android:textColor="#FFFFFF"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="408dp"
        tools:layout_editor_absoluteY="1169dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="110dp"
        android:layout_height="114dp"
        app:srcCompat="@android:drawable/btn_dialog"
        tools:layout_editor_absoluteX="930dp"
        tools:layout_editor_absoluteY="23dp" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="220dp"
        android:layout_height="190dp"
        app:srcCompat="@drawable/logo"
        tools:layout_editor_absoluteX="115dp"
        tools:layout_editor_absoluteY="137dp" />

</androidx.constraintlayout.widget.ConstraintLayout>